{
    "name": "sadam/livewire-range-slider",
    "description": "A feature-rich, customizable range slider component for Laravel Livewire applications, built on noUiSlider with Alpine.js integration",
    "keywords": [
        "laravel",
        "livewire",
        "alpinejs",
        "slider",
        "range-slider",
        "range",
        "blade",
        "tallstack",
        "livewire3",
        "livewire4",
        "laravel11",
        "laravel12",
        "form-components",
        "input-slider",
        "dual-range",
        "multi-range",
        "nouislider",
        "ui-component",
        "interactive-forms",
        "price-slider"
    ],

    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12",
        "livewire/livewire": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sadam\\LivewireRangeSlider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sadam\\LivewireRangeSlider\\LivewireRangeSliderServiceProvider"
            ]
        }
    }
}
